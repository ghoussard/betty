{
  "name": "betty",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "prettier": "prettier --config .prettierrc.json --check src cypress __tests__ jest",
    "prettier:fix": "prettier --config .prettierrc.json --write src cypress __tests__ jest",
    "test:back:unit": "JEST_TEST_SUITE=back:unit jest",
    "test:back:integration": "firebase emulators:exec --only firestore --project test 'JEST_TEST_SUITE=back:integration jest --detectOpenHandles'",
    "test:front": "JEST_TEST_SUITE=front jest",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "firebase emulators:exec --only firestore --project e2e 'start-server-and-test start http://localhost:3000 cypress'",
    "e2e:headless": "firebase emulators:exec --only firestore --project e2e 'start-server-and-test start http://localhost:3000 cypress:headless'"
  },
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "cypress": "^9.4.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
